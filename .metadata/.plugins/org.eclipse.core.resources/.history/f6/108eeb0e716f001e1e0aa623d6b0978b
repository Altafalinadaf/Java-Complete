package ExceptionHandling;


public class printStackTrace {
	static void disp3() {
		int arr[]= {1,23,43,53};
//		System.out.println(arr[9]);
	}
	static void disp2() {
		disp3();
	}
	static void disp1() {
		disp2();
	}
	public static void main(String[] args) {
		try {
			disp1();
		}
		catch (Exception e) {
			// TODO: handle exception
			System.out.println("handled");
		}
	}

}
