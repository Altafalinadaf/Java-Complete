package Java_Programms;

public class LargestAmoungPrime {
	public static void main(String[] args) {
		int count=0;
//		int largest=Integer.MIN_VALUE;
//		int sum=0;
		int num=3;
		for(int i=1;i<=num;i++) {
			if(num%2==0) {
				count++;
			}
		}
		if(count==2) {
			System.out.println("Prime Number ");
		}
		else {
			System.out.println("NOt prime");
		}
	}

}
