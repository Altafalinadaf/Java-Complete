package Java_Programms;

public class LargestAmoungPrime {
	public static void main(String[] args) {
		int count=0;
		int largest=Integer.MIN_VALUE;
		int sum=0
		
		for(int i=2;i<=10;i++) {
			count=0;
			sum=i;
			for(int j=1;j<=sum;j++) {
				if(j%2==0) {
					count++;
				}
			}
			if(count==2) {
//				if(largest<j) {
//					largest=j;
//					System.out.println(largest);
//				}
				System.out.println(sum);
			}
			
		}
		
	
	}

}
